auto-reload yes

bind-key j down
bind-key k up
bind-key j next articlelist
bind-key k prev articlelist
bind-key J next-feed articlelist
bind-key K prev-feed articlelist
bind-key G end
bind-key g home
bind-key d pagedown
bind-key u pageup

browser $BROWSER
macro v set browser "mpv %u" ; open-in-browser ; set browser $BROWSER
